/*Write a code to remove duplicates from an unsorted Linked List */

/* JS*/

class Node {
    constructor(val)
    {
        this.data = val;
        this.next = null;
    }
}


function removeDups(head) {
   
    let curr = head;
    let prev = null;
    
    let set = new Set();
    while (curr!=null){
        if(set.has(curr.data)){
            prev.next = curr.next;
            curr = curr.next;
            
        }else{
            set.add(curr.data);
            prev= curr;
            curr= curr.next;
        }
    }
   
    return head;
};

let head = new Node(12);
head.next = new Node(11);
head.next.next = new Node(12);
head.next.next.next = new Node(21);
head.next.next.next.next = new Node(41);
head.next.next.next.next.next = new Node(43);
head.next.next.next.next.next.next = new Node(21);

console.log(removeDups(head));
